{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  customManagers: [
    {
      customType: 'regex',
      fileMatch: ['^\.github/workflows/.*\.ya?ml$'],
      matchStrings: [
        'docker://(?<depName>[^:@]+):(?<currentValue>[^@\\s]+)(?:@(?<currentDigest>sha256:[a-f0-9]+))?'
      ],
      datasourceTemplate: 'docker'
    }
  ]
}
